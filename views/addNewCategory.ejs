                                    <input type="text" class="form-control" name="uname" placeholder="Enter Your Password Category Name">
                                </div>
                            </div>
                           
                                
                            
                            <div class="form-group">
                                <!-- Button -->                                        
                                <div class="col-md-offset-3 col-md-9">
                                <input type="submit" name="submit" value="Add Category" class="btn btn-primary">   
                                </div>
                            </div>
                             <a href="/passwordcategory">View All Category Lists</a>
                            
                            
                            
                            
                            
                        </form>
